{
	"servers": {
		"devcam": {
			"server": ["_env", "${CAMLI_SERVER}", "http://localhost:3179/"],
			"trustedCerts": [["_env", "${CAMLI_CERT_ID}"]],
			"auth": ["_env", "${CAMLI_AUTH}"],
			"default": true
		}
	},
	"ignoredFiles": [".DS_Store"],
	"identity": ["_env", "${CAMLI_KEYID}"],
	"identitySecretRing": ["_env", "${CAMLI_SECRET_RING}"]
}
